{
  "extends": "airbnb-base",
  "rules": {
    "func-names": ["error", "never"],
    "semi": ["error","never"],
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": ".js"
      }
    }
  },
  "env": {
    "node": true
  }
}